   "source": [
    "function pii()\n",
    "    slices=1000000;\n",
    "    sum =0;\n",
    "    x=0;\n",
    "    delta_x=1/slices;\n",
    "    for i=1:slices\n",
    "        x=(i+0.5)*delta_x\n",
    "        sum = sum + 4/(1+x^2);\n",
    "    end\n",
    "    return pi=sum*delta_x\n",
    "end\n",
    "        "
   ]
